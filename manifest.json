{
  "manifest_version": 3,
  "name": "Salesforce CRM Data Extractor",
  "version": "1.0",
  "description": "Extract Salesforce CRM data",
  "permissions": ["storage", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*.lightning.force.com/*"],
      "js": ["contentScript.js"]
    }
  ]
}
